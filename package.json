{
  "name": "website-shot",
  "type": "module",
  "version": "1.0.0",
  "description": "ðŸ“¸ Generate a full web-page screenshot, Website-shot provides rich interface to make any kind of web screenshots online for free with no limits.",
  "author": "Younes Barrad <younesbarraddev@gmail.com> (https://github.com/Flowko/)",
  "license": "AGPL-3.0-only",
  "repository": "https://github.com/Flowko/website-shot",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "start": "nuxt start",
    "lint": "eslint --ext .js,.vue,.ts,.tsx .",
    "lint:fix": "eslint --fix --ext .js,.vue,.ts,.tsx .",
    "clean": "rm -rf node_modules pnpm-lock.yaml .nuxt && pnpm i",
    "upgrade": "nuxt upgrade --force",
    "db:generate": "pnpm prisma generate",
    "db:push": "pnpm prisma db push",
    "db:drop": "pnpm db:push --force-reset && pnpm db:seed && pnpm db:generate",
    "studio": "pnpm prisma studio --port 5556",
    "db:seed": "pnpm prisma db seed",
    "release": "changelogen --release"
  },
  "devDependencies": {
    "@antfu/eslint-config": "0.43.0-beta.0",
    "@cliqz/adblocker-puppeteer": "^1.26.7",
    "@iconify-json/heroicons": "^1.1.12",
    "@iconify-json/octicon": "^1.1.49",
    "@iconify-json/simple-icons": "^1.1.71",
    "@iconify-json/solar": "^1.1.3",
    "@nuxt/devtools": "^0.8.3",
    "@nuxthq/studio": "^0.14.1",
    "@nuxt/ui": "^2.8.1",
    "@nuxtjs/fontaine": "^0.4.1",
    "@nuxtjs/google-fonts": "^3.0.2",
    "@prisma/client": "^5.3.1",
    "@types/node": "^20.6.2",
    "@vueuse/core": "^10.4.1",
    "@vueuse/nuxt": "^10.4.1",
    "changelogen": "^0.5.5",
    "cron": "^2.4.3",
    "eslint": "8.49.0",
    "nuxt": "^3.7.3",
    "nuxt-headlessui": "^1.1.4",
    "nuxt-icon": "^0.5.0",
    "prisma": "^5.3.1",
    "puppeteer": "^21.2.1",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "sass": "^1.67.0",
    "tailwindcss": "^3.3.3",
    "ts-node": "^10.9.1",
    "ufo": "^1.3.0",
    "zod": "^3.22.2"
  },
  "prisma": {
    "seed": "node --experimental-specifier-resolution=node --loader ts-node/esm ./prisma/seed.ts"
  }
}