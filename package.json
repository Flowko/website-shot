{
  "name": "website-shot",
  "version": "1.0.0",
  "description": "Generate a full web-page screenshot, Website-shot provides rich interface to make any kind of web screenshots online for free with no limits.",
  "repository": "https://github.com/Flowko/website-shot",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "start": "nuxt start",
    "lint": "eslint .",
    "clean": "rm -rf node_modules pnpm-lock.yaml .nuxt && pnpm i",
    "upgrade": "nuxt upgrade --force",
    "db:generate": "pnpm prisma generate",
    "db:push": "pnpm prisma db push",
    "db:drop": "pnpm db:push --force-reset && pnpm db:seed && pnpm db:generate",
    "studio": "pnpm prisma studio --port 5556",
    "db:seed": "pnpm prisma db seed"
  },
  "devDependencies": {
    "@antfu/eslint-config": "0.39.6",
    "@cliqz/adblocker-puppeteer": "^1.26.6",
    "@iconify-json/heroicons": "^1.1.11",
    "@iconify-json/octicon": "^1.1.44",
    "@iconify-json/simple-icons": "^1.1.58",
    "@nuxt/devtools": "^0.6.6",
    "@nuxthq/ui": "^2.5.0",
    "@prisma/client": "^4.16.2",
    "@types/node": "^20.3.2",
    "@types/puppeteer": "^7.0.4",
    "@vueuse/core": "^10.2.1",
    "@vueuse/nuxt": "^10.2.1",
    "eslint": "8.43.0",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "nuxt": "^3.6.1",
    "nuxt-headlessui": "^1.1.4",
    "nuxt-icon": "^0.4.2",
    "prisma": "^4.16.2",
    "puppeteer": "^20.7.4",
    "tailwindcss": "^3.3.2",
    "ts-node": "^10.9.1",
    "ufo": "^1.1.2"
  },
  "eslintConfig": {
    "extends": [
      "plugin:tailwindcss/recommended"
    ]
  },
  "prisma": {
    "seed": "node --experimental-specifier-resolution=node --loader ts-node/esm ./prisma/seed.ts"
  }
}
